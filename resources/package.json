{
  "name": "avengers-vendor-frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "check:stylelint": "stylelint src/**/*.vue",
    "lint:stylelint": "stylelint src/**/*.vue --fix",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "check:eslint": "eslint --ext .vue,.js,.ts ./src",
    "lint:eslint": "eslint ./src --ext .vue,.js,.ts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dayjs": "^1.11.10",
    "element-plus": "^2.4.3",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.13",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "@vueuse/components": "^10.5.0",
    "@vueuse/core": "^10.1.2",
    "@vueuse/integrations": "^10.3.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.28",
    "postcss-html": "^1.5.0",
    "sass": "^1.63.4",
    "stylelint": "^15.11.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "tailwindcss": "^3.3.3",
    "terser": "^5.18.2",
    "typescript": "~5.2.0",
    "unplugin-auto-import": "^0.17.1",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.11",
    "vue-tsc": "^1.8.19"
  }
}
